# Configurações da API
VITE_API_URL=http://localhost:3001/api

# Configurações da aplicação
VITE_APP_NAME=Igreja Admin
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Sistema de Administração de Igreja

# Configurações de desenvolvimento
VITE_DEBUG=false
VITE_LOG_LEVEL=info

# Configurações de autenticação
VITE_TOKEN_STORAGE_KEY=igreja_admin_token
VITE_REFRESH_TOKEN_STORAGE_KEY=igreja_admin_refresh_token

# Configurações de cache
VITE_CACHE_TTL=300000
VITE_QUERY_STALE_TIME=300000

# Configurações de upload
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Configurações de notificação
VITE_NOTIFICATION_DURATION=5000
VITE_SUCCESS_NOTIFICATION_DURATION=3000
VITE_ERROR_NOTIFICATION_DURATION=8000

# Configurações de sessão
VITE_SESSION_TIMEOUT=1800000
VITE_TOKEN_REFRESH_THRESHOLD=300000

# Configurações de tema
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true

# Configurações de idioma
VITE_DEFAULT_LANGUAGE=pt-BR
VITE_SUPPORTED_LANGUAGES=pt-BR,en-US

# Configurações de paginação
VITE_DEFAULT_PAGE_SIZE=10
VITE_MAX_PAGE_SIZE=100

# Configurações de busca
VITE_SEARCH_MIN_LENGTH=2
VITE_SEARCH_DEBOUNCE_DELAY=500

# Configurações de exportação
VITE_EXPORT_MAX_RECORDS=10000
VITE_EXPORT_DEFAULT_FORMAT=xlsx

# Configurações de analytics (opcional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_ENABLE_ANALYTICS=false

# Configurações de monitoramento (opcional)
VITE_SENTRY_DSN=
VITE_ENABLE_ERROR_TRACKING=false
